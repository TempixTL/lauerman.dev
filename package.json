{
  "name": "lauerman.dev",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://lauerman.dev",
  "dependencies": {
    "@fontsource/quicksand": "^4.5.12",
    "normalize.css": "^8.0.1",
    "vanilla-tilt": "^1.8.0"
  },
  "scripts": {
    "clean": "rimraf dist/",
    "prebuild-dev": "npm run clean",
    "build-dev": "eleventy && gulp build",
    "build": "NODE_ENV='production' npm run build-dev",
    "predeploy": "npm run build && echo \"lauerman.dev\" > ./dist/CNAME",
    "deploy": "gh-pages -d dist -b dist",
    "serve": "concurrently -n 11ty,gulp \"eleventy --serve\" \"gulp watch\"",
    "start": "npm run serve"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cssnano": "^5.1.14",
    "gh-pages": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-if": "^3.0.0",
    "gulp-image": "^6.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-responsive": "^3.0.1",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "postcss": "^8.4.21",
    "rimraf": "^4.1.2",
    "sass": "^1.58.0"
  },
  "license": "MIT"
}
