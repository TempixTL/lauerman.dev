{
  "name": "lauerman.dev",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://lauerman.dev",
  "dependencies": {
    "@fontsource/quicksand": "^4.1.0",
    "normalize.css": "^8.0.1",
    "vanilla-tilt": "^1.7.0"
  },
  "scripts": {
    "clean": "rimraf dist/",
    "prebuild-dev": "npm run clean",
    "build-dev": "eleventy && gulp build",
    "build": "NODE_ENV='production' npm run build-dev",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist -b dist",
    "serve": "concurrently -n 11ty,gulp \"eleventy --serve\" \"gulp watch\"",
    "start": "npm run serve"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "autoprefixer": "^10.2.1",
    "concurrently": "^5.3.0",
    "cssnano": "^4.1.10",
    "gh-pages": "^3.1.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-if": "^3.0.0",
    "gulp-image": "^6.2.1",
    "gulp-postcss": "^9.0.0",
    "gulp-responsive": "^3.0.1",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "js-yaml": "^4.0.0",
    "postcss": "^8.2.4",
    "rimraf": "^3.0.2",
    "sass": "^1.32.4"
  },
  "license": "MIT"
}
